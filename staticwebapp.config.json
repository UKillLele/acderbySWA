{
  "platform": {
    "apiRuntime": "node:20"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/public/images/*.{png,jpg,jpeg,gif,bmp}",
      "/css/*"
    ]
  },
  "responseOverrides": {
    "401": {
      "redirect": "/login",
      "statusCode": 302
    }
  },
  "mimeTypes": {
    ".tsx": "application/javascript",
    ".json": "text/json"
  }
}